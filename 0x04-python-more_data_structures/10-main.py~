#!/usr/bin/python3
best_score = __import__('10-best_score').best_score

1_dictionary = {'John': 12, 'Bob': 14, 'Mike': 14, 'Molly': 16, 'Adam': 10}
2_dictionary = {'John': 12}
3_dictionary = {}

best_key = best_score(1_dictionary)
print("Best score: {}".format(best_key))

best_key = best_score(2_dictionary)
print("Best score: {}".format(best_key))

best_key = best_score(3_dictionary)
print("Best score: {}".format(best_key))

best_key = best_score(None)
print("Best score: {}".format(best_key))
